{
  "name": "cwa-assignment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- next dev",
    "build": "cross-env OTEL_LOG_LEVEL=debug VERCEL_OTEL_EXPORTER_OTLP_DISABLED=1 prisma migrate deploy && next build",
    "start": "cross-env OTEL_LOG_LEVEL=debug VERCEL_OTEL_EXPORTER_OTLP_DISABLED=1 next start",
    "lint": "next lint",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "clean": "rimraf .next",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "@vercel/otel": "^2.1.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/webpack": "^5.28.5",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.18.0",
    "rimraf": "^6.1.0",
    "typescript": "^5"
  }
}
